  "test_scenarios": {
    "order_creation_tests": [
      {
        "name": "Valid Single Item Order",
        "description": "Test creating an order with a single product",
        "curl_command": "curl -X POST http://localhost:3000/api/orders/order -H \"Content-Type: application/json\" -H \"Authorization: Bearer YOUR_TOKEN\" -d '{\"user_id\":1,\"store_id\":1,\"total_amount\":75999,\"status\":\"PENDING\",\"order_items\":[{\"product_id\":1,\"quantity\":1,\"price\":75999}]}'",
        "expected_status": 200,
        "expected_response": "Order created successfully with order ID"
      },
      {
        "name": "Valid Multiple Items Order",
        "description": "Test creating an order with multiple products",
        "curl_command": "curl -X POST http://localhost:3000/api/orders/order -H \"Content-Type: application/json\" -H \"Authorization: Bearer YOUR_TOKEN\" -d '{\"user_id\":2,\"store_id\":1,\"total_amount\":268898,\"status\":\"PENDING\",\"order_items\":[{\"product_id\":1,\"quantity\":1,\"price\":75999},{\"product_id\":2,\"quantity\":1,\"price\":134900},{\"product_id\":5,\"quantity\":2,\"price\":12995}]}'",
        "expected_status": 200,
        "expected_response": "Order created successfully with multiple items"
      },
      {
        "name": "Empty Order Data",
        "description": "Test creating an order with empty request body",
        "curl_command": "curl -X POST http://localhost:3000/api/orders/order -H \"Content-Type: application/json\" -H \"Authorization: Bearer YOUR_TOKEN\" -d '{}'",
        "expected_status": 400,
        "expected_response": "Failed to create order due to missing required fields"
      },
      {
        "name": "Missing Authorization",
        "description": "Test creating an order without authorization token",
        "curl_command": "curl -X POST http://localhost:3000/api/orders/order -H \"Content-Type: application/json\" -d '{\"user_id\":1,\"store_id\":1,\"total_amount\":75999,\"status\":\"PENDING\"}'",
        "expected_status": 401,
        "expected_response": "Unauthorized access"
      }
    ],
    "order_retrieval_tests": [
      {
        "name": "Get All Orders",
        "description": "Test retrieving all orders",
        "curl_command": "curl -X GET http://localhost:3000/api/orders -H \"Authorization: Bearer YOUR_TOKEN\"",
        "expected_status": 200,
        "expected_response": "Array of all orders"
      },
      {
        "name": "Get Specific Order",
        "description": "Test retrieving a specific order by ID",
        "curl_command": "curl -X GET http://localhost:3000/api/orders/1",
        "expected_status": 200,
        "expected_response": "Single order object"
      },
      {
        "name": "Get Non-Existent Order",
        "description": "Test retrieving a non-existent order",
        "curl_command": "curl -X GET http://localhost:3000/api/orders/99999",
        "expected_status": 404,
        "expected_response": "Order not found"
      },
      {
        "name": "Get Orders Without Authorization",
        "description": "Test retrieving orders without authorization",
        "curl_command": "curl -X GET http://localhost:3000/api/orders",
        "expected_status": 401,
        "expected_response": "Unauthorized access"
      }
    ],
    "order_update_tests": [
      {
        "name": "Update Order Status",
        "description": "Test updating an order's status",
        "curl_command": "curl -X PUT http://localhost:3000/api/orders/1 -H \"Content-Type: application/json\" -d '{\"status\":\"ACCEPTED\",\"tracking_link\":\"https://tracking.example.com/ORDER001\"}'",
        "expected_status": 200,

  "order_status_flow": {
    "description": "Typical order status flow in the e-commerce system",
    "flow": [
      {
        "status": "PENDING",
        "description": "Order placed by customer, awaiting store approval",
        "next_possible_statuses": ["ACCEPTED", "REJECTED"]
      },
      {
        "status": "ACCEPTED",
        "description": "Store accepted the order and is preparing items",
        "next_possible_statuses": ["READY", "CANCELLED"]
      },
      {
        "status": "REJECTED",
        "description": "Store rejected the order (insufficient stock, etc.)",
        "next_possible_statuses": ["CANCELLED"]
      },
      {
        "status": "READY",
        "description": "Order is ready for pickup/delivery",
        "next_possible_statuses": ["OUT_FOR_DELIVERY", "CANCELLED"]
      },
      {
        "status": "OUT_FOR_DELIVERY",
        "description": "Order has been dispatched and is on the way",
        "next_possible_statuses": ["DELIVERED", "CANCELLED"]
      },
      {
        "status": "DELIVERED",
        "description": "Order successfully delivered to customer",
        "next_possible_statuses": []
      },
      {
        "status": "CANCELLED",
        "description": "Order cancelled (by customer or store)",
        "next_possible_statuses": []
      }
    ]
  }
}

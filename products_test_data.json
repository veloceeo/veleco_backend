  "api_endpoints_test_data": {
    "description": "Test data for different product API endpoints",
    "get_all_products": {
      "endpoint": "GET /products/",
      "headers": {
        "Authorization": "Bearer YOUR_JWT_TOKEN"
      },
      "expected_response": "Array of all products"
    },
    "add_product": {
      "endpoint": "POST /products/add",
      "headers": {
        "Authorization": "Bearer YOUR_JWT_TOKEN",
        "Content-Type": "application/json"
      },
      "sample_request_body": {
        "product_name": "Test Product",
        "price": "99",
        "product_img": ["https://example.com/image1.jpg", "https://example.com/image2.jpg"],
        "quantity": "25",
        "category": "Test Category",
        "stock": "25"
      }
    },
    "check_quantity_and_stock": {
      "endpoint": "POST /products/quantity/:name",
      "headers": {
        "Authorization": "Bearer YOUR_JWT_TOKEN"
      },
      "sample_requests": [
        {
          "product_name": "iPhone 15 Pro Max",
          "expected_response": {
            "product_name": "iPhone 15 Pro Max",
            "quantity": 50,
            "stock": 50
          }
        },
        {
          "product_name": "Limited Edition Sneakers",
          "expected_response": "low stock"
        },
        {
          "product_name": "NonExistent Product",
          "expected_response": {
            "error": "Product not found"
          }
        }
      ]
    },
    "search_product": {
      "endpoint": "GET /products/search/:name",
      "sample_searches": [
        {
          "search_term": "iphone",
          "description": "Case-insensitive search for iPhone products"
        },
        {
          "search_term": "nike",
          "description": "Search for Nike products"
        },
        {
          "search_term": "laptop",
          "description": "Search for laptop/MacBook products"
        },
        {
          "search_term": "nonexistent",
          "expected_response": {
            "error": "Product not found"
          }
        }
      ]
    },
    "delete_product": {
      "endpoint": "DELETE /products/:name",
      "headers": {
        "Authorization": "Bearer YOUR_JWT_TOKEN"
      },
      "sample_requests": [
        {
          "product_name": "Test Product to Delete",
          "expected_response": {
            "message": "Product deleted successfully",
            "name": "Test Product to Delete"
          }
        },
        {
          "product_name": "NonExistent Product",
          "expected_response": {
            "error": "Product not found"
          }
        }
      ]
    }
  },
  "curl_examples": {
    "description": "cURL commands for testing product endpoints",
    "get_all_products": "curl -X GET \"http://localhost:3000/products/\" -H \"Authorization: Bearer YOUR_JWT_TOKEN\"",
    "add_product": "curl -X POST \"http://localhost:3000/products/add\" -H \"Authorization: Bearer YOUR_JWT_TOKEN\" -H \"Content-Type: application/json\" -d '{\"product_name\":\"iPhone 15 Pro Max\",\"price\":\"1199\",\"product_img\":[\"https://images.unsplash.com/photo-1592750475338-74b7b21085ab\"],\"quantity\":\"50\",\"category\":\"Electronics\",\"stock\":\"50\"}'",
    "check_quantity": "curl -X POST \"http://localhost:3000/products/quantity/iPhone%2015%20Pro%20Max\" -H \"Authorization: Bearer YOUR_JWT_TOKEN\"",
    "search_product": "curl -X GET \"http://localhost:3000/products/search/iphone\"",
    "delete_product": "curl -X DELETE \"http://localhost:3000/products/iPhone%2015%20Pro%20Max\" -H \"Authorization: Bearer YOUR_JWT_TOKEN\""
  },
  "postman_collection_samples": {
    "description": "Ready-to-import Postman collection data",
    "collection_name": "E-Commerce Products API",
    "requests": [
      {
        "name": "Get All Products",
        "method": "GET",
        "url": "{{base_url}}/products/",
        "headers": [
          {
            "key": "Authorization",
            "value": "Bearer {{jwt_token}}"
          }
        ]
      },
      {
        "name": "Add Product",
        "method": "POST",
        "url": "{{base_url}}/products/add",
        "headers": [
          {
            "key": "Authorization",
            "value": "Bearer {{jwt_token}}"
          },
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"product_name\": \"iPhone 15 Pro Max\",\n  \"price\": \"1199\",\n  \"product_img\": [\"https://images.unsplash.com/photo-1592750475338-74b7b21085ab\"],\n  \"quantity\": \"50\",\n  \"category\": \"Electronics\",\n  \"stock\": \"50\"\n}"
        }
      },
      {
        "name": "Check Product Quantity",
        "method": "POST",
        "url": "{{base_url}}/products/quantity/iPhone 15 Pro Max",
        "headers": [
          {
            "key": "Authorization",
            "value": "Bearer {{jwt_token}}"
          }
        ]
      },
      {
        "name": "Search Product",
        "method": "GET",
        "url": "{{base_url}}/products/search/iphone"
      },
      {
        "name": "Delete Product",
        "method": "DELETE",
        "url": "{{base_url}}/products/iPhone 15 Pro Max",
        "headers": [
          {
            "key": "Authorization",
            "value": "Bearer {{jwt_token}}"
          }
        ]
      }
    ],
    "variables": [
      {
        "key": "base_url",
        "value": "http://localhost:3000"
      },
      {
        "key": "jwt_token",
        "value": "YOUR_JWT_TOKEN_HERE"
      }
    ]
  },
  "edge_cases_and_error_scenarios": {
    "description": "Test cases for error handling and edge cases",
    "invalid_data_scenarios": [
      {
        "scenario": "Missing required fields",
        "request_body": {
          "product_name": "Test Product"
        },
        "expected_error": "Database validation error or missing fields"
      },
      {
        "scenario": "Invalid price format",
        "request_body": {
          "product_name": "Test Product",
          "price": "invalid_price",
          "quantity": "10",
          "category": "Test",
          "stock": "10"
        },
        "expected_error": "parseInt will return NaN"
      },
      {
        "scenario": "Negative stock",
        "request_body": {
          "product_name": "Test Product",
          "price": "99",
          "quantity": "-5",
          "category": "Test",
          "stock": "-5"
        },
        "expected_behavior": "Should be handled by business logic"
      },
      {
        "scenario": "Empty product name",
        "request_body": {
          "product_name": "",
          "price": "99",
          "quantity": "10",
          "category": "Test",
          "stock": "10"
        },
        "expected_error": "Validation error"
      }
    ],
    "authorization_scenarios": [
      {
        "scenario": "No JWT token",
        "headers": {},
        "expected_error": "401 Unauthorized"
      },
      {
        "scenario": "Invalid JWT token",
        "headers": {
          "Authorization": "Bearer invalid_token"
        },
        "expected_error": "401 Unauthorized"
      },
      {
        "scenario": "User without store",
        "description": "User tries to add product but doesn't have a store",
        "expected_error": "Store not found for this user"
      }
    ]
  },

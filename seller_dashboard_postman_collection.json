  "event": [
    {
      "listen": "prerequest",
      "script": {
        "type": "text/javascript",
        "exec": [
          "// Auto-set timestamps for dynamic testing",
          "pm.globals.set('current_date', new Date().toISOString().split('T')[0]);",
          "pm.globals.set('current_datetime', new Date().toISOString());",
          "",
          "// Generate random test data",
          "pm.globals.set('random_phone', '+1-555-' + Math.floor(Math.random() * 9000 + 1000));",
          "pm.globals.set('random_price', Math.floor(Math.random() * 500 + 50));"
        ]
      }
    },
    {
      "listen": "test",
      "script": {
        "type": "text/javascript",
        "exec": [
          "// Common test assertions",
          "pm.test('Status code is successful', function () {",
          "    pm.expect(pm.response.code).to.be.oneOf([200, 201]);",
          "});",
          "",
          "pm.test('Response has message field', function () {",
          "    pm.expect(pm.response.json()).to.have.property('message');",
          "});",
          "",
          "pm.test('Response time is reasonable', function () {",
          "    pm.expect(pm.response.responseTime).to.be.below(5000);",
          "});"
        ]
      }
    }
  ]
}

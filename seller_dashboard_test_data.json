  "curl_examples": {
    "get_dashboard_overview": "curl -X GET http://localhost:3000/api/seller/dashboard/overview -H 'Authorization: Bearer YOUR_SELLER_TOKEN'",
    "get_today_analytics": "curl -X GET http://localhost:3000/api/seller/analytics/today -H 'Authorization: Bearer YOUR_SELLER_TOKEN'",
    "open_store": "curl -X PUT http://localhost:3000/api/seller/store/toggle-status -H 'Content-Type: application/json' -H 'Authorization: Bearer YOUR_SELLER_TOKEN' -d '{\"action\":\"open\"}'",
    "close_store": "curl -X PUT http://localhost:3000/api/seller/store/toggle-status -H 'Content-Type: application/json' -H 'Authorization: Bearer YOUR_SELLER_TOKEN' -d '{\"action\":\"close\"}'",
    "generate_daily_report": "curl -X POST http://localhost:3000/api/seller/reports/generate -H 'Content-Type: application/json' -H 'Authorization: Bearer YOUR_SELLER_TOKEN' -d '{\"report_type\":\"DAILY\",\"start_date\":\"2024-06-24\",\"end_date\":\"2024-06-24\"}'",
    "get_inventory_alerts": "curl -X GET http://localhost:3000/api/seller/inventory/alerts?resolved=false&priority=HIGH -H 'Authorization: Bearer YOUR_SELLER_TOKEN'",
    "get_notifications": "curl -X GET http://localhost:3000/api/seller/notifications?unread_only=true -H 'Authorization: Bearer YOUR_SELLER_TOKEN'"
  }

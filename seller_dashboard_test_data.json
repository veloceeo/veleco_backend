  "dashboard_api_endpoints": {
    "description": "API endpoints for seller dashboard functionality",
    "endpoints": [
      {
        "name": "Get Dashboard Overview",
        "method": "GET",
        "url": "/api/seller/dashboard/overview",
        "description": "Get today's sales, orders count, and inventory summary",
        "auth_required": true,
        "response_fields": [
          "today_sales_amount",
          "today_orders_count",
          "live_inventory_count",
          "pending_orders",
          "low_stock_alerts"
        ]
      },
      {
        "name": "Get Today's Analytics",
        "method": "GET",
        "url": "/api/seller/analytics/today",
        "description": "Get detailed analytics for today",
        "auth_required": true,
        "response_fields": [
          "daily_sales_amount",
          "daily_orders_count",
          "total_customers_served",
          "average_order_value",
          "hourly_sales_breakdown"
        ]
      },
      {
        "name": "Toggle Store Status",
        "method": "PUT",
        "url": "/api/seller/store/toggle-status",
        "description": "Open or close store",
        "auth_required": true,
        "request_body": {
          "action": "open|close"
        }
      },
      {
        "name": "Generate Sales Report",
        "method": "POST",
        "url": "/api/seller/reports/generate",
        "description": "Generate sales report for specified period",
        "auth_required": true,
        "request_body": {
          "report_type": "DAILY|WEEKLY|MONTHLY",
          "start_date": "2024-06-01",
          "end_date": "2024-06-30"
        }
      },
      {
        "name": "Get Inventory Alerts",
        "method": "GET",
        "url": "/api/seller/inventory/alerts",
        "description": "Get all active inventory alerts",
        "auth_required": true,
        "query_params": {
          "resolved": "false",
          "priority": "HIGH|CRITICAL"
        }
      },
      {
        "name": "Mark Alert as Resolved",
        "method": "PUT",
        "url": "/api/seller/inventory/alerts/:alertId/resolve",
        "description": "Mark an inventory alert as resolved",
        "auth_required": true
      },
      {
        "name": "Get Dashboard Notifications",
        "method": "GET",
        "url": "/api/seller/notifications",
        "description": "Get seller notifications",
        "auth_required": true,
        "query_params": {
          "unread_only": "true|false",
          "urgent_only": "true|false"
        }
      },
      {
        "name": "Mark Notification as Read",
        "method": "PUT",
        "url": "/api/seller/notifications/:notificationId/read",
        "description": "Mark notification as read",
        "auth_required": true
      }
    ]
  },
